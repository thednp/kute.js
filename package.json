{
  "name": "kute.js",
  "version": "2.2.5",
  "author": "thednp",
  "license": "MIT",
  "description": "JavaScript animation engine",
  "homepage": "http://thednp.github.io/kute.js",
  "type": "module",
  "main": "dist/kute.min.js",
  "module": "dist/kute.esm.js",
  "jsnext": "src/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "rolldown --help",
    "build": "pnpm lint:js && pnpm run \"/^(copy-build|build-.*)$/\"",
    "custom": "rolldown -c --environment",
    "format": "deno fmt src docs/*html",
    "fix:js": "deno lint src --fix",
    "lint:js": "deno lint src",
    "badges": "npx -p dependency-version-badge update-badge typescript @thednp/bezier-easing svg-path-commander",
    "copy-build": "rolldown --environment OUTPUTFILE:docs/src/kute.min.js,DIST:standard,MIN:true,FORMAT:umd -c",
    "build-standard": "rolldown --environment DIST:standard,MIN:false,FORMAT:umd -c",
    "build:ts": "tsc -d",
    "build-standard-min": "rolldown --environment DIST:standard,MIN:true,FORMAT:umd -c",
    "build-standard-esm": "rolldown --environment DIST:standard,MIN:false,FORMAT:esm -c",
    "build-standard-esm-min": "rolldown --environment DIST:standard,MIN:true,FORMAT:esm -c",
    "build-base": "rolldown --environment OUTPUTFILE:docs/src/kute-base.js,DIST:base,MIN:false,FORMAT:umd -c",
    "build-base-min": "rolldown --environment OUTPUTFILE:docs/src/kute-base.min.js,DIST:base,MIN:true,FORMAT:umd -c",
    "build-extra": "rolldown --environment OUTPUTFILE:docs/src/kute-extra.js,DIST:extra,MIN:false,FORMAT:umd -c",
    "build-extra-min": "rolldown --environment OUTPUTFILE:docs/src/kute-extra.min.js,DIST:extra,MIN:true,FORMAT:umd -c",
    "prepublishOnly": "pnpm update --latest && pnpm badges && pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thednp/kute.js.git"
  },
  "keywords": [
    "kute.js",
    "svg morph",
    "svg transform",
    "css3 transform",
    "matrix transform",
    "tweening engine",
    "animation engine",
    "javascript animation engine",
    "javascript animation",
    "animation",
    "native javascript"
  ],
  "bugs": {
    "url": "https://github.com/thednp/kute.js/issues"
  },
  "dependencies": {
    "@thednp/bezier-easing": "^1.0.11",
    "@thednp/shorty": "^2.0.11",
    "minifill": "^0.0.17",
    "svg-path-commander": "^2.1.11"
  },
  "devDependencies": {
    "rolldown": "1.0.0-rc.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.6.0"
  }
}
